pipeline{
    agent any
    stages{
        stage('checkout the code from github'){
            steps{
                 git url: 'https://github.com/balu777kb/banking-finance.git/'
                 echo 'github url checkout'
            }
        }
        stage('codecompile with balu'){
            steps{
                echo 'starting compiling'
                sh 'mvn compile'
            }
        }
        
     }
}
